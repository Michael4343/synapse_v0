# Development-specific environment variables for faster performance
NODE_ENV=development

# Next.js development optimizations
NEXT_PRIVATE_SKIP_TRACE=true
NEXT_PRIVATE_DISABLE_STATIC_IMAGES=true

# Turbopack optimizations
TURBOPACK=1
NEXT_TELEMETRY_DISABLED=1

# WSL2 optimizations
WATCHPACK_POLLING=true
CHOKIDAR_USEPOLLING=true